library(ggplot2)
views <- data.frame(
Season = c(1, 2, 3, 4, 5, "6A", "6B"),
Episodes = c(10, 10, 10, 10, 10, 7, 6),
First_Aired = as.Date(c("2015-02-08", "2016-02-15", "2017-04-10", "2018-08-06", "2020-02-23", "2022-04-18", "2022-07-11")),
Last_Aired = as.Date(c("2015-04-06", "2016-04-18", "2017-06-19", "2018-10-08", "2020-04-20", "2022-05-23", "2022-08-15")),
Avg_Viewers = c(3.21, 2.16, 1.64, 1.49, 1.37, 1.27, 1.27)
)
ggplot(views, aes(x = Season, y = Avg_Viewers, group = 1)) +
geom_line() +
geom_point() +
labs(x = "Season", y = "Average Viewers (millions)", title = "Average Viewership Over Seasons")
